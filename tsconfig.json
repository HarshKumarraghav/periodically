{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "pages/index.tsx",
    "pages/_app.ts",
    "context/userContext.tsxx",
    "context/userContext.tsxx",
    "components/Header.tsxx",
    "utils/SortData.ts",
    "utils/Debounce.ts",
    "utils/Debounce.ts"
  ],
  "exclude": ["node_modules"]
}
